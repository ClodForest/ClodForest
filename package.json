{
  "name": "claudelink-coordinator",
  "version": "1.0.0",
  "description": "ClaudeLink Coordinator Service - Provides active protocol support for distributed Claude instance coordination",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test.js",
    "install-service": "sudo cp claudelink-coordinator.service /etc/systemd/system/ && sudo systemctl daemon-reload",
    "enable-service": "sudo systemctl enable claudelink-coordinator",
    "start-service": "sudo systemctl start claudelink-coordinator",
    "stop-service": "sudo systemctl stop claudelink-coordinator",
    "status-service": "sudo systemctl status claudelink-coordinator",
    "logs": "sudo journalctl -u claudelink-coordinator -f"
  },
  "keywords": [
    "claudelink",
    "ai-coordination", 
    "distributed-systems",
    "context-protocol",
    "repository-management"
  ],
  "author": "Robert de Forest <robert@defore.st>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdeforest/claude-code-bundler.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
   "bugs": {
    "url": "https://github.com/rdeforest/claude-code-bundler/issues"
  },
  "homepage": "https://github.com/rdeforest/claude-code-bundler#readme",
  "devDependencies": {
    "coffeescript": "^2.7.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rdeforest/claudelink-coordinator.git"
  },
  "config": {
    "port": 8080,
    "freebsd_server": "claudelink-vault",
    "repo_base_path": "/public/file/0/git",
    "github_repo_path": "/public/file/0/git/claude-code-bundler"
  }
}
